{
  "name": "@chapanda/style-preset-interactive-client",
  "version": "1.4.0",
  "description": "chapanda style preset interactive document client",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "vite build --emptyOutDir --watch",
    "dev:client": "vite dev",
    "build": "vite build --emptyOutDir",
    "build:docs": "cross-env NODE_ENV_IS_DOCS=true vite build --emptyOutDir",
    "preview": "vite preview",
    "publish:npm": "pnpm publish --no-git-checks --access public"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./index.js",
      "import": "./index.js"
    },
    "./index.css": "./index.scss",
    "./index.js": "./index.js"
  },
  "files": [
    "DIN-Bold.otf",
    "DINNextLTPro-Medium.ttf",
    "DINNextLTPro-Bold.ttf",
    "DINNextLTPro-Heavy.ttf",
    "index.html",
    "index.js",
    "index.css",
    "README.md"
  ],
  "dependencies": {
    "@ant-design/icons": "^5.5.1",
    "@chapanda/style-preset": "1.4.0",
    "antd": "^5.21.6",
    "baiwusanyu-utils": "1.0.18",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tinycolor2": "^1.6.0",
    "unocss": "^0.64.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/tinycolor2": "^1.4.6",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "cross-env": "^7.0.3",
    "typescript": "^5.6.3",
    "vite": "^5.4.10"
  }
}
