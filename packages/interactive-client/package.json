{
  "name": "@chapanda/style-preset-interactive-client",
  "version": "0.0.3",
  "description": "chapanda style preset interactive document client",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "vite build --emptyOutDir --watch",
    "dev:client": "vite dev",
    "build": "vite build --emptyOutDir",
    "preview": "vite preview",
    "publish:npm": "pnpm publish --no-git-checks --access public"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./index.js",
      "import": "./index.js"
    },
    "./index.css": "./index.scss",
    "./index.js": "./index.js"
  },
  "files": [
    "index.html",
    "index.js",
    "index.css",
    "README.md"
  ],
  "dependencies": {
    "@chapanda/style-preset": "workspace: *",
    "baiwusanyu-utils": "1.0.18",
    "tinycolor2": "^1.6.0",
    "unocss": "^0.62.3",
    "antd": "^5.20.1",
    "@ant-design/icons": "^5.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/tinycolor2": "^1.4.6",
    "typescript": "^5.5.3",
    "vite": "^5.4.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.7.0"
  }
}
